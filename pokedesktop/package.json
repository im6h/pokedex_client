{
	"name": "pokedesktop",
	"version": "0.1.1",
	"author": "im6h",
	"private": true,
	"dependencies": {
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.5.0",
		"@testing-library/user-event": "^7.2.1",
		"@types/jest": "^24.9.1",
		"@types/node": "^12.12.54",
		"@types/react": "^16.9.47",
		"@types/react-dom": "^16.9.8",
		"@types/react-router": "^5.1.8",
		"@types/react-router-dom": "^5.1.5",
		"@types/styled-components": "^5.1.3",
		"antd": "^4.6.5",
		"axios": "^0.20.0",
		"cross-env": "^7.0.2",
		"mobx": "^5.15.6",
		"mobx-react-lite": "^2.1.0",
		"node-sass": "^4.14.1",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-router": "^5.2.0",
		"react-router-dom": "^5.2.0",
		"react-scripts": "3.4.3",
		"react-web-vector-icons": "^1.0.2",
		"styled-components": "^5.2.0",
		"typescript": "^3.7.5"
	},
	"scripts": {
		"start:web": "react-scripts start",
		"build:web": "cross-env PUBLIC_URL=./ react-scripts build",
		"build:desktop": "tsc -p electron/tsconfig.json",
		"start:desktop": "npm run build:desktop && electron ./build/electron.js",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"postinstall": "install-app-deps",
		"pack": "electron-builder --dir",
		"dist": "electron-builder"
	},
	"build": {
		"appId": "io.pokedex.app",
		"win": {
			"target": "NSIS",
			"icon": "build/icon.ico"
		}
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [">0.2%", "not dead", "not op_mini all"],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"electron": "^10.0.0",
		"electron-builder": "^22.8.1",
		"electron-installer-windows": "^3.0.0"
	}
}
